{
  "include": ["index.ts"],
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "bundler",
    "lib": ["es2015", "dom"],
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "allowImportingTsExtensions":false,
    "baseUrl": ".",
    "paths": {
      "greenlet": ["./node_modules/greenlet/index.d.ts"],
      "tslib" : ["./node_modules/tslib/tslib.d.ts"]
    }
  }
}
