{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "strictNullChecks": false,
    "paths": { "@/*":["./src/*"] },
    "allowSyntheticDefaultImports": true,
    "types": ["element-plus/global", "node"],
    "skipLibCheck": true,
    "noImplicitAny": false
  },
  "include": ["./src/**/*.ts", "./src/**/*.d.ts", "./src/**/*.tsx", "./src/**/*.vue", "test/**/*.test.ts"],
}